        "from math import *\n",
        "def square_root(number, guess):\n",
        "    i= 0\n",
        "    while i<100:\n",
        "        guess = (-(guess**2-number)/(2*guess) + guess)\n",
        "        i+=1\n",
        "        print(guess)\n",
        "    return guess\n",
        "x = square_root(1000, 610)\n",
        "print(x)\n",
        "print(x**2)"
      ],
